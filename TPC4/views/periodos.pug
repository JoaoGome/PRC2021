extends layout 

block content 
    .w3-card-4 

        if (info.length)
            .w3-container 
                table(class="w3-table-all")
                    tr 
                        th Período  
                    each i in info 
                        tr 
                            td
                                a(href=`/periodo/${element}/${group}/${period}/${i}`) #{i}
                            
        
        else 
            div(class="w3-container")
                p Não há Periodos!